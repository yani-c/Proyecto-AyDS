//------------------------------------------- cargar usuario  y borrar usuario curl -------------------------------------------------------------------------------

curl -X POST http://localhost:4567/users  -H 'content-type: application/json'   -d '{"name":"Alan","password":"Turing","dni":"37127650","administrator":true}'

curl -X POST http://localhost:4567/deleteuser  -H 'content-type: application/json'   -d '{"id" : "1" }'

//-----------------------------------------------------------------------------------------------------------------------------------------------------------------




//-------------------------------- cargar categorias curl ------------------------------------------------------------------ 

curl -X POST http://localhost:4567/categories  -H 'content-type: application/json'   -d '{"category_name":"Biologia"}'

curl -X POST http://localhost:4567/categories  -H 'content-type: application/json'   -d '{"category_name":"huesos"}'

